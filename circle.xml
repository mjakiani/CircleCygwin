  <target name="init" description="Build initialization">
    <!-- Perform any build initialization in this target -->
  </target>
  
  <target name="clean" depends="clean.module.circle" description="cleanup all"/>
  
  <target name="build.modules" depends="init, clean, compile.module.circle" description="build all modules"/>
  
  <target name="init.artifacts">
    <property name="artifacts.temp.dir" value="${basedir}/__artifacts_temp"/>
    <property name="artifact.output.circle:jar" value="${basedir}/out/artifacts/Circle_jar"/>
    <mkdir dir="${artifacts.temp.dir}"/>
    <property name="temp.jar.path.Circle.jar" value="${artifacts.temp.dir}/Circle.jar"/>
  </target>
  
  <target name="artifact.circle:jar" depends="init.artifacts, compile.module.circle" description="Build &#39;Circle:jar&#39; artifact">
    <property name="artifact.temp.output.circle:jar" value="${artifacts.temp.dir}/Circle_jar"/>
    <mkdir dir="${artifact.temp.output.circle:jar}"/>
    <jar destfile="${temp.jar.path.Circle.jar}" duplicate="preserve" filesetmanifest="mergewithoutmain">
      <zipfileset file="${basedir}/src/main/resources/META-INF/MANIFEST.MF" prefix="META-INF"/>
      <zipfileset dir="${circle.output.dir}"/>
    </jar>
    <copy file="${temp.jar.path.Circle.jar}" tofile="${artifact.temp.output.circle:jar}/Circle.jar"/>
  </target>
  
  <target name="build.all.artifacts" depends="artifact.circle:jar" description="Build all artifacts">
    <mkdir dir="${artifact.output.circle:jar}"/>
    <copy todir="${artifact.output.circle:jar}">
      <fileset dir="${artifact.temp.output.circle:jar}"/>
    </copy>
    
    <!-- Delete temporary files -->
    <delete dir="${artifacts.temp.dir}"/>
  </target>
  
  <target name="all" depends="build.modules, build.all.artifacts" description="build all"/>
